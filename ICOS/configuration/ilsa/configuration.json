{
  ":configurator:resource-version": 1,
  ":configurator:symbolic-name": "org.eclipse.sensinact.gateway.configuration",
  ":configurator:version": "0.0.1",
  "sensinact.launcher": {
    "features": [
      "base-emf",
      "core-feature",
      "gogo-shell-feature",
      "jakarta-servlet-whiteboard-feature",
      "jakarta-rest-whiteboard-feature",
      "northbound-rest-feature",
      "configuration-interpolation",
      "de.jena.sensinact.icos.ilsa.runtime-feature"
    ],
    "repository": [
      "repository",
      "ext-repository"
    ],
    "featureDir": [
      "features",
      "ext-features"
    ]
  },
  "org.apache.felix.http": {
    "org.osgi.service.http.port": 8080,
    "org.apache.felix.http.name": "sensiNact"
  },
  "JakartarsServletWhiteboardRuntimeComponent": {
    "osgi.jakartars.name": "sensiNact.rest",
    "osgi.http.whiteboard.target": "(org.apache.felix.http.name=sensiNact)"
  },
  "sensinact.northbound.rest": {
    "allow.anonymous": true
  },
  "sensinact.session.manager": {
    "auth.policy": "ALLOW_ALL"
  },
  "MQTTService~read": {
    "brokerUrl": "ssl://$[env:MQTT_ICOS_HOST;default=$[prop:MQTT_ICOS_HOST;default=mqttbr.jena.de]]:$[env:MQTT_ICOS_PORT;default=$[prop:MQTT_ICOS_PORT;default=8883]]",
    "username": "$[env:MQTT_ICOS_USER;default=]$[prop:MQTT_ICOS_USER;default=]",
    "password": "$[secret:icos_password;default=]$[env:MQTT_ICOS_PWD;default=]$[prop:MQTT_ICOS_PWD;default=]",
    "maxInflight": 1000,
    "id": "read"
  },
  "MQTTService~full": {
    "brokerUrl": "tcp://$[env:MQTT_DIM_HOST;default=$[prop:MQTT_DIM_HOST;default=datainmotion.de]]:$[env:MQTT_DIM_PORT;default=$[prop:MQTT_DIM_PORT;default=1883]]",
    "username": "$[env:MQTT_DIM_USER;default=]$[prop:MQTT_DIM_USER;default=]",
    "password": "$[secret:dim_password;default=]$[env:MQTT_DIM_PWD;default=]$[prop:MQTT_DIM_PWD;default=]",
    "maxInflight": 1000,
    "id": "full"
  }
}